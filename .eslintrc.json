{
  "extends": [
    "airbnb/base",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "root": true,
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "expect": true,
    "it": true,
    "describe": true,
    "Drupal": true
  },
  "rules": {
    "strict": 0,
    "prettier/prettier": "error"
  }
}
